<html>
  <head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Bootstrap -->
    <link href="/css/bootstrap.min.css" rel="stylesheet" media="screen">
    <link href="/css/bootstrap-theme.min.css" rel="stylesheet" media="screen">
    <link href="/css/datepicker.css" rel="stylesheet" media="screen">
    <link href="/css/aptomar.css" rel="stylesheet" media="screen">
  </head>
  <body>
    <script src="/js/jquery-min.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    <script src="/js/bootstrap-datepicker.js"></script>
    <div class="jumbotron span12">
      <h1><a href='/'>AMQP Examples</a></h1>

      <p class="muted">Simple frontend to inspect the messages going by...</p>

    </div>
    <div class="container span12">

      <div class="message row">
        <table class="table table-responsive table-condensed table-hover">
          <thead>
            <tr>
              <th>Message recv'd</th>
       	      <th>Contents</th>
            </tr>
          </thead>
          <tbody>
            <% for(var i=0; i < messages.length; i++) { %>
               <tr>
                 <td width="30%"><%= messages[i].timestamp %></td>
                 <td width="70%"><pre><%= messages[i].message %></pre></td>
               </tr>
            <% } %>
          </tbody>
        </table>
      </div>
    </div>
  </body>
</html>
